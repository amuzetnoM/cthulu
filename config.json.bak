{
  "_comment": "Ultra-Aggressive profile with dynamic strategy selection",
  "mt5": {
    "login": "${MT5_LOGIN}",
    "password": "${MT5_PASSWORD}",
    "server": "${MT5_SERVER}",
    "timeout": 60000,
    "path": "${MT5_PATH}"
  },
  "risk": {
    "max_position_size": 2.0,
    "default_position_size": 0.1,
    "max_daily_loss": 500.0,
    "max_positions_per_symbol": 3,
    "max_total_positions": 10,
    "position_size_pct": 15.0,
    "use_kelly_sizing": true,
    "emergency_stop_loss_pct": 12.0,
    "circuit_breaker_enabled": true,
    "circuit_breaker_threshold_pct": 7.0
  },
  "trading": {
    "symbol": "GOLDm#",
    "timeframe": "TIMEFRAME_M15",
    "poll_interval": 15,
    "lookback_bars": 500,
    "enable_tick_data": true,
    "enable_scalping": true
  },
  "strategy": {
    "type": "dynamic",
    "dynamic_selection": {
      "enabled": true,
      "regime_check_interval": 180,
      "min_strategy_signals": 3,
      "performance_weight": 0.4,
      "regime_weight": 0.4,
      "confidence_weight": 0.2
    },
    "strategies": [
      {
        "type": "ema_crossover",
        "params": {
          "fast_period": 8,
          "slow_period": 21,
          "atr_period": 14,
          "atr_multiplier": 1.2,
          "risk_reward_ratio": 3.0,
          "symbol": "GOLDm#"
        }
      },
      {
        "type": "momentum_breakout",
        "params": {
          "lookback_period": 12,
          "rsi_threshold": 45,
          "atr_multiplier": 1.3,
          "volume_multiplier": 1.3,
          "risk_reward_ratio": 3.5,
          "symbol": "GOLDm#"
        }
      },
      {
        "type": "scalping",
        "params": {
          "fast_ema": 5,
          "slow_ema": 10,
          "rsi_period": 7,
          "rsi_oversold": 20,
          "rsi_overbought": 75,
          "rsi_long_max": 70,
          "rsi_short_min": 30,
          "atr_multiplier": 0.8,
          "risk_reward_ratio": 2.5,
          "spread_limit_pips": 5.0,
          "symbol": "GOLDm#"
        }
      },
      {
        "type": "trend_following",
        "params": {
          "adx_threshold": 15,
          "fast_ma": 8,
          "slow_ma": 21,
          "atr_period": 14,
          "atr_multiplier": 1.5,
          "risk_reward_ratio": 2.0,
          "symbol": "GOLDm#"
        }
      },
      {
        "type": "sma_crossover",
        "params": {
          "fast_period": 5,
          "slow_period": 13,
          "atr_period": 14,
          "atr_multiplier": 1.5,
          "risk_reward_ratio": 2.5,
          "symbol": "GOLDm#"
        }
      },
      {
        "type": "mean_reversion",
        "params": {
          "ma_period": 20,
          "bb_std": 2.0,
          "rsi_period": 14,
          "rsi_oversold": 28,
          "rsi_overbought": 72,
          "atr_multiplier": 1.2,
          "risk_reward_ratio": 2.5,
          "symbol": "GOLDm#"
        }
      },
      {
        "type": "rsi_reversal",
        "params": {
          "rsi_period": 14,
          "rsi_extreme_oversold": 28,
          "rsi_extreme_overbought": 72,
          "atr_multiplier": 1.5,
          "risk_reward_ratio": 2.0,
          "cooldown_bars": 2,
          "symbol": "GOLDm#"
        }
      }
    ]
  },
  "confidence_threshold":0.15,
  "exit_strategies": [
    {
      "type": "adverse_movement",
      "enabled": true,
      "params": {
        "movement_threshold_pct": 0.5,
        "time_window_seconds": 30
      }
    },
    {
      "type": "time_based",
      "enabled": true,
      "params": {
        "max_hold_hours": 8.0,
        "close_before_weekend": true
      }
    },
    {
      "type": "profit_target",
      "enabled": true,
      "params": {
        "target_pct": 1.5,
        "partial_close_enabled": true,
        "partial_close_pct": 50.0
      }
    },
    {
      "type": "trailing_stop",
      "enabled": true,
      "params": {
        "atr_multiplier": 1.5,
        "activation_pct": 0.5
      }
    }
  ],
  "indicators": [
    {
      "type": "ema",
      "params": {
        "period": 5
      }
    },
    {
      "type": "ema",
      "params": {
        "period": 8
      }
    },
    {
      "type": "ema",
      "params": {
        "period": 9
      }
    },
    {
      "type": "ema",
      "params": {
        "period": 10
      }
    },
    {
      "type": "ema",
      "params": {
        "period": 21
      }
    },
    {
      "type": "sma",
      "params": {
        "period": 5
      }
    },
    {
      "type": "sma",
      "params": {
        "period": 13
      }
    },
    {
      "type": "rsi",
      "params": {
        "period": 14
      }
    },
    {
      "type": "rsi",
      "params": {
        "period": 7
      }
    },
    {
      "type": "macd",
      "params": {
        "fast_period": 12,
        "slow_period": 26,
        "signal_period": 9
      }
    },
    {
      "type": "bollinger",
      "params": {
        "period": 20,
        "std_dev": 2.0
      }
    },
    {
      "type": "adx",
      "params": {
        "period": 14
      }
    },
    {
      "type": "supertrend",
      "params": {
        "period": 10,
        "multiplier": 3.0
      }
    },
    {
      "type": "vwap",
      "params": {
        "std_dev_multiplier": 2.0
      }
    }
  ],
  "database": {
    "path": "Cthulu_ultra_aggressive.db"
  },
  "rpc": {
    "enabled": true,
    "host": "127.0.0.1",
    "port": 8278
  },
  "features": {
    "ml_enabled": true,
    "adaptive_parameters": true,
    "multi_timeframe_analysis": true
  },
  "dynamic_sltp": {
    "enabled": true,
    "base_sl_atr_mult": 2.0,
    "base_tp_atr_mult": 4.0,
    "breakeven_activation_pct": 0.5,
    "breakeven_buffer_pct": 0.1,
    "partial_tp_enabled": true,
    "partial_tp_1_pct": 0.33,
    "partial_tp_2_pct": 0.66,
    "trail_activation_pct": 0.7,
    "trail_distance_atr_mult": 1.5
  },
  "adaptive_drawdown": {
    "enabled": true,
    "trailing_lock_pct": 0.5,
    "survival_threshold_pct": 50.0,
    "critical_threshold_pct": 35.0,
    "danger_threshold_pct": 20.0
  },
  "entry_confluence": {
    "enabled": true,
    "min_score_to_enter": 30,
    "min_score_for_full_size": 60,
    "enable_wait_mode": true,
    "max_wait_bars": 5,
    "force_entry_on_timeout": true,
    "timeout_size_mult": 0.5,
    "sr_lookback": 100,
    "sr_min_touches": 2,
    "level_tolerance_atr": 0.8,
    "momentum_bars": 5,
    "momentum_weight": 0.20,
    "level_weight": 0.30,
    "timing_weight": 0.25,
    "structure_weight": 0.25
  },
  "hektor": {
    "enabled": true,
    "fallback_to_sqlite": true,
    "database_path": "./vectors/cthulu_memory",
    "dimension": 512,
    "hnsw_m": 16,
    "hnsw_ef_construction": 200,
    "hnsw_ef_search": 50,
    "default_k": 10,
    "min_similarity_score": 0.7,
    "max_context_age_days": 90,
    "batch_size": 100,
    "async_writes": true,
    "cache_embeddings": true,
    "connection_timeout_ms": 5000,
    "retry_attempts": 3
  }
}