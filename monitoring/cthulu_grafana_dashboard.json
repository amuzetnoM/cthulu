{
  "dashboard": {
    "title": "Cthulu - System Health & Trading Performance",
    "panels": [
      {
        "type": "row",
        "title": "Overview"
      },
      {
        "type": "stat",
        "title": "Uptime (s)",
        "metric": "Cthulu_uptime_seconds",
        "description": "System uptime in seconds"
      },
      {
        "type": "graph",
        "title": "Net PnL",
        "metrics": [
          "Cthulu_pnl_total",
          "Cthulu_profit_total",
          "Cthulu_loss_total"
        ]
      },
      {
        "type": "graph",
        "title": "Trade Counts",
        "metrics": [
          "Cthulu_trades_total",
          "Cthulu_trades_won",
          "Cthulu_trades_lost"
        ]
      },
      {
        "type": "graph",
        "title": "Win Rate & Profit Factor",
        "metrics": [
          "Cthulu_win_rate",
          "Cthulu_profit_factor"
        ]
      },
      {
        "type": "table",
        "title": "Per-Symbol Breakdown",
        "metrics_template": "Cthulu_symbol_realized_pnl{symbol=~\"$symbol\"}, Cthulu_symbol_open_positions{symbol=~\"$symbol\"}"
      },
      {
        "type": "row",
        "title": "Signals & Orders"
      },
      {
        "type": "graph",
        "title": "Signals Generated (per minute)",
        "metrics": [
          "cthulu_signals_generated_total"
        ]
      },
      {
        "type": "table",
        "title": "Recent Orders/Provenance",
        "metrics": [
          "cthulu_orders_total",
          "cthulu_orders_filled_total",
          "cthulu_orders_rejected_total"
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "symbol",
          "type": "query",
          "query": "label_values(Cthulu_symbol_realized_pnl, symbol)"
        }
      ]
    }
  },
  "notes": "This is a scaffold dashboard JSON. Import into Grafana and map panels to Prometheus queries. Panel types and queries will be finalized after the metrics schema is validated."
}